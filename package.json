{
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.15.6",
  "name": "yev-starter",
  "engines": {
    "node": ">= 18",
    "pnpm": ">= 8.15"
  },
  "description": "",
  "scripts": {
    "bootstrap": "pnpm i && turbo stub",
    "create-app": "esno ./scripts/create-app.mts",
