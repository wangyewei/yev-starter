    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@yev-stater/tsconfig": "workspace:*",
    "@yev-stater/uno-config": "workspace:*",
    "@yev-stater/vite-config": "workspace:*",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "esno": "^4.8.0",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "unocss": "^66.0.0",
    "vite": "^6.2.2"
  },
  "dependencies": {
    "@unocss/reset": "^66.0.0"
  }
